{
  "name": "@markdown-di/core",
  "version": "0.9.12",
  "description": "Parser-agnostic dependency injection for markdown using frontmatter",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target node --format esm && tsc --emitDeclarationOnly",
    "test": "bun test",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "markdown",
    "frontmatter",
    "dependency-injection",
    "validation",
    "parser-agnostic"
  ],
  "author": "Pepijn Senders",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/PepijnSenders/markdown-di.git",
    "directory": "packages/core"
  },
  "dependencies": {
    "@types/mustache": "4.2.6",
    "ajv": "^8.12.0",
    "ajv-formats": "^3.0.1",
    "fast-glob": "^3.3.3",
    "gray-matter": "^4.0.3",
    "mustache": "4.2.0",
    "yaml": "^2.7.0"
  },
  "peerDependencies": {
    "zod": "^4.1.12"
  },
  "peerDependenciesMeta": {
    "zod": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.9.3"
  }
}
