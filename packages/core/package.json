{
  "name": "@markdown-di/core",
  "version": "0.9.11",
  "description": "Parser-agnostic dependency injection for markdown using frontmatter",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target node --format esm && tsc --emitDeclarationOnly",
    "test": "bun test",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "markdown",
    "frontmatter",
    "dependency-injection",
    "validation",
    "parser-agnostic"
  ],
  "author": "Pepijn Senders",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/PepijnSenders/markdown-di.git",
    "directory": "packages/core"
  },
  "dependencies": {
    "@types/mustache": "catalog:",
    "ajv": "catalog:",
    "ajv-formats": "catalog:",
    "fast-glob": "catalog:",
    "gray-matter": "catalog:",
    "mustache": "catalog:",
    "yaml": "catalog:"
  },
  "peerDependencies": {
    "zod": "catalog:"
  },
  "peerDependenciesMeta": {
    "zod": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/bun": "catalog:",
    "typescript": "catalog:"
  }
}
